<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xs.assistant.service.user.DAO.UserInfoDAO">
    <select id="getAllCustomer" resultType="com.xs.DAO.customer.DO.CustomerDO">
        SELECT
            Id,FirstName,LastName,Email,Birth,IdNumber,AreaId
        FROM
            customer
    </select>

    <select id="selectCustomer" resultType="com.xs.DAO.customer.DO.CustomerDO">
        SELECT
            Id,FirstName,LastName,Email,Birth,IdNumber,AreaId
        FROM
            customer
        WHERE
            Id = #{id}
    </select>


</mapper>