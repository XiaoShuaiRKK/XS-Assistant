<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xs.assistant.service.user.DAO.UserUpdateDAO">

    <insert id="insertCustomer" parameterType="com.xs.DAO.DO.customer.CustomerDO">
        INSERT INTO
            customer
        VALUES
            (NULL,#{customer.firstName},#{customer.lastName},#{customer.email},
             #{customer.password},#{customer.birth},#{customer.idNumber},#{customer.areaId},0,now())
    </insert>

</mapper>